#lang racket
(require "crear.rkt")
(define (hallarm vec m cont)
  (if (< cont (- (vector-length vec) 1))
      (if (> (vector-ref vec cont) (vector-ref vec m))
          (hallarm vec cont (+ cont 1))
          (hallarm vec m (+ cont 1)))
      m))
